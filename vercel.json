{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.3" }
    },
    "builds": [
        {
        "src": "public/build/*",
        "use": "@vercel/static"
        },
        {
        "src": "public/assets/*",
        "use": "@vercel/static"
        },
        {
        "src": "index.php",
        "use": "@vercel/php"
        }
    ],
    "routes": [
        { 
            "src": "/build/(.*)", 
            "dest": "/public/build/$1" 
        },
        { 
            "src": "/assets/(.*)", 
            "dest": "/public/assets/$1" 
        },
        { 
            "src": "/(.*)", 
            "dest": "/api/index.php"
        }
    ],
